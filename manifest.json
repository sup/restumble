{
	"manifest_version": 2,
	"name": "Restumble",
	"version": "1.0",
	"description": "StumbleUpon for your own bookmarks",
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "img/icon48.png",
			"38": "img/icon48.png"
		}
	},
	"web_accessible_resources": [
		"img/icon128.png"
	],
	"permissions": [
		"bookmarks"
	],
	"background": {
        "scripts": ["js/bookmarks.js"]
    }
}
